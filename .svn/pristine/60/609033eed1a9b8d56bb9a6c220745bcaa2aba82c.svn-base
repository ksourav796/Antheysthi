<div class="modal fade" id="add_new_payment_modal">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span></button>
                <!--<h4 id="user-title">Add Assign</h4>-->
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-md-12 col-sm-12">
                        <form class="form-horizontal" name="mail">
                            <input type="hidden" ng-model="setupobj.rid" value="{{setupobj.rid}}"/>

                            <fieldset>
                                <!--<div class="form-group">-->
                                    <!--<label class="col-md-4 col-sm-4 control-label" for="">Mode Of Payment</label>-->
                                    <!--<div class="col-md-6 col-sm-6" >-->
                                        <!--<select name="paymentmode" id="paymentmode" ng-model="paymentmode" class="form-control input-md"-->
                                                <!--ng-options="paymentMethod.paymentmethodName as paymentMethod.paymentmethodName for paymentMethod in paymentMethodList">-->
                                        <!--</select>-->
                                    <!--</div>-->
                                <!--</div>-->
                                <input type="hidden" ng-model="setupobj.rid" value="{{setupobj.rid}}"/>
                                <div class="form-group">

                                    <label class="col-md-4 col-sm-4 control-label" for="">Actual Price</label>
                                    <div class="col-md-6 col-sm-6" style="position:relative;">
                                        <input id="totalAmount" name="actualPrice"
                                               type="text" placeholder="actualPrice"
                                               ng-model="actualPrice" ng-change="getBalTotal()"
                                               class="form-control input-md" autocomplete="off">

                                    </div>
                                </div><div class="form-group">
                                <label class="col-md-4 col-sm-4 control-label" for="">Amount Paid</label>
                                <div class="col-md-6 col-sm-6" style="position:relative;">
                                    <input id="totalAmount" name="totalAmount"
                                           type="text" placeholder="totalAmount"
                                           ng-model="totalAmount" ng-change="getBalTotal()"
                                           ng-model-options="{ debounce: 500 }"
                                           class="form-control input-md" autocomplete="off">

                                </div>
                            </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="">Amount Balance</label>
                                    <div class="col-md-6">
                                        <input id="totalAmount" name="balAmount"
                                               type="text" placeholder="balAmount"
                                               ng-model="balAmount"
                                               class="form-control input-md" autocomplete="off">
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="control-label col-sm-4" for="">Payment Date:</label>
                                    <div class="col-sm-8">
                                        <p class="input-group">
                                            <input type="text" class="form-control" uib-datepicker-popup="{{format}}" ng-model="transactionDate" is-open="popup1.opened" datepicker-options="dateOptions" ng-required="true" close-text="Close" alt-input-formats="altInputFormats" />
                                            <span class="input-group-btn">
                                     <button type="button" class="btn btn-default" ng-click="open1()"><i class="glyphicon glyphicon-calendar"></i></button>
                                </span>
                                        </p>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 col-sm-4 control-label" for="">Status</label>
                                    <div class="col-md-6 col-sm-6" >
                                        <select name="status" id="status" ng-model="active" ng-init="active='Approved'" class="form-control input-md">
                                            <option value="approved">Approved</option>
                                            <option value="rejected">Rejected</option>
                                            <option value="suspended">Suspended</option>
                                        </select>
                                    </div>
                                </div>
                            </fieldset>
                        </form>
                    </div>
                </div>
            </div>
            <div class="modal-footer">

                <button type="submit" class="btn btn-default" data-toggle="modal"
                        data-target=".print-me"  ng-click="savepayment(setupobj.rid)">Save
                </button>
                <button type="button" class="btn btn-default" ng-click="removepayments()" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
