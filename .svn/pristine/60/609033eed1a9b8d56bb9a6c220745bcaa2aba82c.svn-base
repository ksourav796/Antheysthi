<div class="modal fade Student" tabindex="-1" id="View_Details" role="dialog"
     data-backdrop="static"
     aria-labelledby="mySmallModalLabel">

    <div class="modal-dialog modal-lg" style="width: 1000px !important;background-color: white !important;">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span></button>
        </div>
        <div align="center">


            <br/>
            <br/>

            <div class="row">
                <div class="col-lg-9 admission_border">

                    <table class="admission_table_1 table">
                        <tbody>
                        <tr>
                            <td colspan="2" style="padding-left: 20px;text-align:center;
">
                                <h3 >Restaurant Details</h3>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="adm_title_half">1.RName</span>
                                <span class="adm_colon">:</span>
                                <span class="adm_val_half" style="width: 56%;">{{rName}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="adm_title_half">2.Date </span>
                                <span class="adm_colon">:</span>
                                <span class="adm_val_half" style="width: 56%;">{{date}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="adm_title_half">3.Location </span>
                                <span class="adm_colon">:</span>
                                <span class="adm_val_half" style="width: 56%;">{{location}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="adm_title_half">4.Email </span>
                                <span class="adm_colon">:</span>
                                <span class="adm_val_half" style="width: 56%;">{{email}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="adm_title_half">5.Contact </span>
                                <span class="adm_colon">:</span>
                                <span class="adm_val_half" style="width: 56%;">{{contact}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="adm_title_half">6.CName </span>
                                <span class="adm_colon">:</span>
                                <span class="adm_val_half" style="width: 56%;">{{cName}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="adm_title_half">7.Address </span>
                                <span class="adm_colon">:</span>
                                <span class="adm_val_half" style="width: 56%;">{{address}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="adm_title_half">8.City </span>
                                <span class="adm_colon">:</span>
                                <span class="adm_val_half" style="width: 56%;">{{city}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="adm_title_half">9.Zipcode </span>
                                <span class="adm_colon">:</span>
                                <span class="adm_val_half" style="width: 56%;">{{zipcode}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="adm_title_half">10.Remarks </span>
                                <span class="adm_colon">:</span>
                                <span class="adm_val_half" style="width: 56%;">{{remarks}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="adm_title_half">11.Product </span>
                                <span class="adm_colon">:</span>
                                <span class="adm_val_half" style="width: 56%;">{{product}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="adm_title_half">12.Service </span>
                                <span class="adm_colon">:</span>
                                <span class="adm_val_half" style="width: 56%;">{{service}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="adm_title_half">13.User </span>
                                <span class="adm_colon">:</span>
                                <span class="adm_val_half" style="width: 56%;">{{user}}</span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span class="adm_title_half">14.GstNo </span>
                                <span class="adm_colon">:</span>
                                <span class="adm_val_half" style="width: 56%;">{{gstNo}}</span>
                            </td>
                        </tr>

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

