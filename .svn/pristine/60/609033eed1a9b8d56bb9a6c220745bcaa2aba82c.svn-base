<div class="modal fade" id="add_new_res_modal">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span></button>
                <h4 id="res-title">Add Restaurant</h4>
            </div>
            <div class="modal-body">
                <div class="box-body">
                    <div class="row" id="cus_step1">
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Product</label>
                            <div class="col-sm-9 " style="position:relative;">
                                <select  name="product" ng-model="categoryId" ng-options="category.categoryName
                                as category.categoryName for category in categoryList"
                                         ng-change="CategoryBasedOnService(categoryId)"
                                         class="form-control input-md ng-change='getFirstLevel(currencyId)">
                                    <option value="">Select</option>
                                </select>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-sm-3  control-label">Service</label>
                            <div class=" col-sm-9" style="position:relative;">
                                <select  name="service" ng-model="service" ng-options="service.name
                                as service.name for service in serviceList"
                                         class="form-control input-md ng-change='getFirstLevel(currencyId)">
                                    <option value="">Select</option>
                                </select>
                            </div>
                        </div>

                        <div class="form-group">
                            <label  class="col-sm-3 control-label">Name</label>
                            <div class="col-sm-9">
                                <input id="Rname" name="Name" alpha-without-space=""
                                       type="text" placeholder="Restaurant Name"
                                       ng-model="Name" maxlength="125"
                                       class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label  class="col-sm-3 control-label">RestaurantName</label>
                            <div class="col-sm-9">
                                <input id="Rname" name="Rname" alpha-without-space=""
                                       type="text" placeholder="Restaurant Name"
                                       ng-model="Rname" maxlength="125"
                                       class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label  class="col-sm-3 control-label">Email</label>
                            <div class="col-sm-9">
                                <input id="Email" name="Email"
                                       type="text" placeholder="Email"
                                       ng-model="Emailid" maxlength="125"
                                       class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label  class="col-sm-3 control-label">Contact</label>
                            <div class="col-sm-9">
                                <input id="Contact" name="Contact" num-with-out-space=""
                                       type="text" placeholder="Contact"
                                       ng-model="Contact" maxlength="125"
                                       class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Country</label>
                            <div class="col-sm-9">
                                <select id="countryId" name="countryId" ng-model="country"  ng-change="countryState(country)" ng-options="country.countryId as country.countryName for country in countryList" class="form-control input-md">
                                    <option value="">Select</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label" for="">State</label>
                            <div class="col-sm-9" style="position:relative;">
                                <select id="state" name="state"
                                        ng-change="stateCity(state)"
                                        ng-model="state"
                                        ng-options="Branches.stateId as Branches.stateName for Branches in stateList"
                                        class="form-control input-md">
                                    <option value="">Select</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label" for="">City</label>
                            <div class="col-sm-9" style="position:relative;">
                                <select id="city" name="city"
                                        ng-model="city"
                                        ng-options="Branches.cityName as Branches.cityName for Branches in cityList"
                                        class="form-control input-md">
                                    <option value="">Select</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label  class="col-sm-3 control-label">Location</label>
                            <div class="col-sm-9">
                                <input id="Location" name="Location" alpha-without-space=""
                                       type="text" placeholder="Location"
                                       ng-model="Location" maxlength="125"
                                       class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label  class="col-sm-3 control-label">AddressLine1</label>
                            <div class="col-sm-9">
                                <input id="Address1" name="Address1" alphanum-with-space=""
                                       type="text" placeholder="Address"
                                       ng-model="address1" maxlength="125"
                                       class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label  class="col-sm-3 control-label">AddressLine2</label>
                            <div class="col-sm-9">
                                <input id="Address1" name="Address2" alphanum-with-space=""
                                       type="text" placeholder="Address"
                                       ng-model="address2" maxlength="125"
                                       class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label  class="col-sm-3 control-label">Zipcode</label>
                            <div class="col-sm-9">
                                <input id="Zipcode" name="Zipcode" num-with-out-space=""
                                       type="text" placeholder="Zipcode"
                                       ng-model="Zipcode" maxlength="125"
                                       class="form-control">
                            </div>
                        </div>
                    </div>

                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-toggle="modal" ng-disabled="isDisabled"
                        data-target=".print-me" ng-click="saveRes('add_new_res_modal')">Save</button>
                <button type="button" class="btn btn-default" data-dismiss="modal" ng-click="removeCountryDetails()">Close</button>

            </div>
        </div>

    </div>
</div>

<div class="modal fade" id="add_new_Status_modal">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span></button>
                <h4 id="status-title">Add Status</h4>
            </div>
            <div class="modal-body">
                <div class="box-body">
                    <div class="col-md-8">
                        <div class="col-md-4">
                            <input type="radio" id="excel" ng-model="type" name="excel" value="ExcelCreation" checked> ExcelWork
                        </div>
                        <div class="col-md-4">
                            <input type="radio" id="company" ng-model="type" name="company" value="CompanyCreation"> CompanyCreation
                        </div>
                        <div class="col-md-4">
                            <input type="radio" id="qrCode" ng-model="type" name="qrCode" value="QRCode"> QRCode
                        </div>

                    </div>
                    </fieldset>
                    </form>
                    <div class="modal-footer">
                        <button type="submit" class="btn btn-primary hi_btn" data-toggle="modal" ng-disabled=""
                                data-target=".print-me" ng-disabled="!form.$valid" id="submit"
                                ng-click="saveNew()">Save
                        </button>
                        <button type="button" class="btn btn-primary hi_btn" ng-click="removeUpload()"
                                data-dismiss="modal">Close
                        </button>

                    </div>

                </div>

            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="add_new_upload_modal">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span></button>
                <h4 id="state-title">Attach file</h4>
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-md-12 col-sm-12">
                        <form class="form-horizontal" name="form">
                            <fieldset>
                                <!--<div class="row">-->
                                <!--<h2 class="pos-header-new">-->
                                <!--<div><strong>Upload Logo</strong></div>-->
                                <!--</h2>-->
                                <!--</div>-->

                                <div class="form-group">
                                    <div class="col-md-6" ng-repeat="f in fileList" style="margin-left: 96px !important;">
                                        <label class="col-md-4 col-sm-4 control-label">File</label>
                                        <div  class="col-md-4">
                                            <input type="file" onchange="angular.element(this).scope().showFileName(event)" ng-click="setId(f.id)"/>
                                            <img class="image-preview"
                                                 style="width: 61px;height:50px;margin-left: 275px;" src={{files}}>
                                            <br><br />
                                        </div>
                                        <div style="text-align:right; width:155%;" ng-if="fileList.length>1 && $index!=0" >
                                            <input type="button"  value="remove"  ng-click="removeFile($index)">
                                        </div>
                                    </div>
                                    <div class="col-md-2">
                                        <input type="button" value="Add New"
                                               ng-click="addNewFile()">
                                    </div>
                                </div>
                                    <div class="form-group">
                                        <label class="col-md-4 col-sm-4 control-label" for="">Status</label>
                                        <div class="col-md-6 col-sm-6" >
                                        <select name="status" id="status" ng-model="uploadStatus"  class="form-control input-md">
                                        <option value="Menu">Menu</option>
                                        <option value="Company Creation">Company Creation</option>
                                        <option value="QR">QR</option>
                                        <option value="Excel">Excel</option>
                                        </select>
                                        </div>
                                </div>

                            </fieldset>
                        </form>

                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="submit" class="btn btn-primary hi_btn" data-toggle="modal" ng-disabled=""
                        data-target=".print-me" ng-disabled="!form.$valid" id="submit"
                        ng-click="saveUpload('')">Save
                </button>
                <button type="button" class="btn btn-primary hi_btn" ng-click="removeUpload()"
                        data-dismiss="modal">Close
                </button>

            </div>

        </div>
    </div>
</div>

