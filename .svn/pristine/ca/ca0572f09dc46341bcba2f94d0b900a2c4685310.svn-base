<section class="content" style="background-color: #fff;">
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="preview-area">
                <h2></h2>
                <div class="" ng-repeat="key in previewList">
                    <!--For Date-->
                    <div class="{{key.classwidth}}" ng-show="key.type=='date'">
                        <!-- for Date Top-->
                        <div class="form-group" ng-show="key.labelposition=='top'"  >
                            <label class="" for="email" ng-show="key.type=='date'">{{key.field}}:</label><span style="color: red" ng-if="key.validate">*</span>
                            <div class="">
                                <input type="date" class="form-control" id="email"  name="email">
                            </div>
                            <p class="desc">{{key.changedesc}}</p>
                        </div>
                        <!-- for Date Left (left-aligned)-->
                        <div class="form-group" ng-show="key.labelposition=='left(left-aligned)'" >
                            <label class="" for="email" ng-show="key.type=='date'" style="float: left; width: 30%; margin-right: 3%; text-align: left;">{{key.field}}:</label><span style="color: red" ng-if="key.validate">*</span>
                            <div class="" style="width: 67%; margin-left: 33%;">
                                <input type="date" class="form-control" id="email"  name="email">
                            </div>
                            <p class="desc">{{key.changedesc}}</p>
                        </div>
                        <!-- for Date Left (right-aligned)-->
                        <div class="form-group" ng-show="key.labelposition=='left(right-aligned)'" >
                            <label class="" for="email" ng-show="key.type=='date'" style="float: left; width: 30%; margin-right: 3%; text-align: right;">{{key.field}}:</label><span style="color: red" ng-if="key.validate">*</span>
                            <div class="" style="width: 67%; margin-left: 33%;">
                                <input type="date" class="form-control" id="email"  name="email">
                            </div>
                            <p class="desc">{{key.changedesc}}</p>
                        </div>
                        <!-- for Date right (left-aligned)-->
                        <div class="form-group" ng-show="key.labelposition=='left(right-aligned)'" >
