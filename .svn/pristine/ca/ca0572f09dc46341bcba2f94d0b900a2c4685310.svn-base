                            <div class="" style="width: 67%; margin-left: 33%;">
                                <input type="date" class="form-control" id="email"  name="email">
                            </div>
                            <p class="desc">{{key.changedesc}}</p>
                        </div>
                        <!-- for Date botton -->
                        <div class="form-group" ng-show="key.labelposition=='bottom'" >
                            <div class="" style="width: 67%; margin-left: 33%;">
                                <input type="date" class="form-control" id="email"  name="email">
                            </div>
                            <label class="" for="email" ng-show="key.type=='date'" style="float: right; width: 30%; margin-right: 3%; text-align: right;">{{key.field}}:</label><span style="color: red" ng-if="key.validate">*</span>
                            <p class="desc">{{key.changedesc}}</p>
                        </div>
                    </div>
                    <!-- for Input -->
                    <div class="{{key.classwidth}}" ng-show="key.type=='input'">
                        <!-- for Input Top-->
                        <div class="form-group" ng-show="key.labelposition=='top'"  >
                            <label class="" for="email" ng-show="key.type=='input'" ng-hide="key.hideLabel==true">{{key.field}}:</label><span style="color: red" ng-if="key.validate">*</span>
                            <i class="fa fa-question-circle text-muted" data-toggle="tooltip" ng-if="key.tooltip" title="{{key.tooltip}}" data-placement="right"></i>
