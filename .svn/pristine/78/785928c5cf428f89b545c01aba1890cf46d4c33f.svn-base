<style type="text/css">
    .preview-area{
        height: 300px;
        overflow-y: scroll;
    }
    .define-data{
        height: 300px;
        overflow-y: scroll;
    }
    .form-group {
        margin-bottom: 15px;
    }
    .nav-tabs-custom>.nav-tabs>li{
        margin-right: -6px;
    }
</style>
<section class="content-header">
    <h1>
        {{operation}} Create Page
    </h1>

</section>

<section class="content define-data">
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">

                <div class="box-body">
                    <div class="row" id="cus_step1">
                        <div class="col-md-4 col-sm-4 col-xs-12">
                            <div class="form-group">
                                <label  class=" control-label">Module Name<span style="color: red">*</span></label>
                                <div class="">
                                    <select id="module" name="module" ng-model="moduleName" ng-change="getSubmoduleList(moduleName)" ng-options="module.moduleName as
											module.moduleName for module in moduleList" class="form-control">
                                        <option value="">Create New</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4 col-sm-4 col-xs-12">
                            <div class="form-group">
                                <label  class=" control-label">Sub-Module Name</label>
                                <div class="">
                                    <select id="module" name="module" ng-model="submoduleName" ng-options="submodule.subModuleName as
											submodule.subModuleName for submodule in subModuleList"  class="form-control">
                                        <option value="">Create New</option>
                                    </select>
                            </div>
                        </div>
                        </div>
                        <div class="col-md-4 col-sm-4 col-xs-12">
                            <div class="form-group">
                                <label  class="control-label">Page Name</label>
                                <div class="">
                                    <input id="pageName" name="pageName"
                                           type="text" placeholder="Page Name" alpha-without-space
                                           ng-model="pageName" maxlength="125"
                                           class="form-control">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div ng-repeat="column in NoOfColumnsList" ng-init="parentIndex=$index">
                                <div class="form-group" style="width: 29%;margin-left: 30px;">
                                    <label  class="control-label">No.Of Columns<span style="color: red">*</span></label>
                                    <div class="">
                                        <input id="columns" name="columns" ng-change="getColumnsList(column.columns,column,$index+1)"
                                               type="text" placeholder="Enter No.Of Columns" num-with-out-space
                                               ng-model="column.columns" maxlength="1" readonly
                                               class="form-control">
                                        <p class="error" ng-show="column.columns==4" style="color: red">Maximum Columns Reached</p>
                                    </div>

                                    <div class=""
                                         ng-hide="column.columns==4" style="float: right;margin-top: -32px;margin-right: -35px;">
                                        <input type="button" value="+"
                                               ng-click="increaseColumns(column,$index+1);getColumnsList(column.columns,column,$index+1);preview()">
                                    </div>
                                </div>
                    <div ng-repeat="details in column.detailsList">
                        <div class="col-sm-4">
                            <div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">Column</label>
                                    <div class="col-sm-10">
                                            <input type="email" class="form-control input-md" id="column" readonly  name="email" ng-model="details.noOfColumn">

                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-4" >
                            <div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">Field</label>
                                    <div class="col-sm-10">
                                        <input id="field" name="field"
                                               type="text" placeholder="Enter Field Name" ng-change="preview()" maxlength="30"
                                               ng-model="details.field" alpha-without-space
                                               class="form-control input-md">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-4" style="margin-top: 15px;">

                            <div class="form-group">
                                <label class="col-sm-2 control-label">Type</label>
                                <div class="col-sm-10" style="padding: 0px;">
                                    <div class="col-sm-12" style="padding: 0px;">
                                        <div class="col-sm-9">
                                            <select id="typeOfEnquiryList" name="typeOfEnquiryList" ng-model="details.type" ng-change="preview()"   class="form-control input-md">
                                                <option value="">Select Type</option>
                                                <option value="date">Date</option>
                                                <option value="input">Input</option>
                                                <option value="number">Number</option>
                                                <option value="password">Password</option>
                                                <option value="textarea">Textarea</option>
                                                <option value="radio">Radio</option>
                                                <option value="checkbox">Checkbox</option>
                                                <option value="select">Select</option>
                                            </select>
                                        </div>
                                        <div class="col-sm-1" ng-if="details.type" style="margin-left: -20px;margin-top: 5px;" >
                                            <a><i class="fa fa-cog" aria-hidden="true" ng-click="openSettings(details,parentIndex,$index)"></i></a>

                                        </div>
                                        <div class="col-sm-1"
                                             ng-if="column.detailsList.length>1 && $index!=0" style="margin-left: -10px;">
                                            <input type="button" value="X" ng-click="removedetailsList(column,$index);preview()">
                                        </div>
                                        <div class="col-sm-1"
                                             ng-if="column.detailsList.length-1==$index">
                                            <input type="button" value="+"
                                                   ng-click="addNewdetailsList($index,column)">
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                    </div>
                    </div>
                </div>
                </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-toggle="modal" ng-disabled="isDisabled"
                        data-target=".print-me" ng-click="saveCreatePage()">Save
                </button>
                <button type="button" class="btn btn-default" data-dismiss="modal" ng-click="closeForm()">Close</button>
            </div>
        </div>
    </div>
</section>
