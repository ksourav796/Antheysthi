<div class="panel-body">
    <div class="row">

        <div class="col-md-12 col-sm-12">
            <form class="form-horizontal" name="enquiry">
                <fieldset>
                    <div class="box-body">
                        <h2 id="enquiry-title" style="padding-top: 0px;margin-top: 0px;margin-bottom: 25px;"> Enquiry Form </h2>

                            <div class="col-md-6 col-sm-6 col-xs-12 form-group">
                                <label class="control-label col-sm-3" style="padding: 0px;">Enquiry Date</label>
                                <div class="col-sm-9">
                                    <div class="input-group date">
                                    <input type="text" class="form-control" uib-datepicker-popup="{{format}}"
                                    ng-model="enquiryDate" style="height: 35px;"
                                    is-open="popup2.opened" readonly datepicker-options="dateOptions"
                                    ng-required="true"
                                    close-text="Close" alt-input-formats="altInputFormats"/>
                                    <span class="input-group-btn">
                                     <button type="button" class="btn btn-default" ng-click="open2()"><i
                                             class="glyphicon glyphicon-calendar"></i></button>
                                   </span>
                                </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-6 col-xs-12 form-group">
                                <label class="control-label col-sm-3" style="padding: 0px;">Enquiry No</label>
                                <div class="col-sm-9">
                                    <input id="enquiryNo" name="enquiryNo" readonly ng-model="enquiryNo" class="form-control">
                                </div>
                            </div>
                            <div style="clear: both"></div>
                            <div class="col-md-6 col-sm-6 col-xs-12 form-group">
                                <div class="col-sm-3" style="padding: 0px;">
                                    <label class="control-label" >First Name<span style="color: red">*</span></label>
                                </div>
                                <div class="col-sm-9">
                                    <input id="firstName" name="firstName"
                                           alpha-with-space
                                           type="text" placeholder="First Name" ng-model-options="{debounce:1000}"
                                           ng-model="firstName" maxlength="125"
                                           class="form-control">
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-6 col-xs-12 form-group">
                                <div class="col-sm-3" style="padding: 0px;">
                                    <label class="control-label" >Last Name</label>
                                </div>
                                <div class="col-sm-9">
                                    <input id="LastName" name="LastName"
                                           alpha-with-space
                                           type="text" placeholder="Last Name"
                                           ng-model="lastName" maxlength="125"
                                           class="form-control">
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-6 col-xs-12 form-group">
                                <div class="col-sm-3" style="padding: 0px;">
                                    <label class="control-label" >Caller Location<span style="color: red">*</span></label>
                                </div>
                                <div class="col-sm-9">
                                    <input id="callerLocation" name="callerLocation"
                                           type="text" placeholder="CallerLocation"
                                           ng-model="callerLocation" maxlength="125"
                                           class="form-control">
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-6 col-xs-12 form-group">
                                <div class="col-sm-3" style="padding: 0px;">
                                    <label class="control-label" >Service Location<span style="color: red">*</span></label>
                                </div>
                                <div class="col-sm-9">
                                    <select id="serviceLocation" name="serviceLocation" ng-model="serviceLocation"
                                            ng-options="serviceLocation.servicelName as serviceLocation.servicelName for serviceLocation in serviceLocationList"
                                            class="form-control input-md">
                                        <option value="">Select</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-6 col-xs-12 form-group">
                                <div class="col-sm-3" style="padding: 0px;">
                                    <label class="control-label" >Source Of Enquiry<span style="color: red">*</span></label>
                                </div>
                                <div class="col-sm-9">
                                    <select id="sourceOfEnquiry" name="sourceOfEnquiry" ng-model="sourceOfEnquiry"
                                            ng-options="sourceOfEnquiry.sourceName as sourceOfEnquiry.sourceName for sourceOfEnquiry in sourceOfEnquiryList"
                                            class="form-control input-md">
                                        <option value="">Select</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-6 col-xs-12 form-group">
                                <div ng-repeat="phone in phoneNumberList">
                                    <div class="col-sm-3" style="padding: 0px;">
                                        <label class="control-label" >Phone Number<span style="color: red">*</span></label>
                                    </div>
                                    <div class="col-sm-9" style="padding: 0px;margin-bottom: 10px;">

                                        <div class="col-md-12 col-sm-12 col-xs-12" style="padding: 0px;">
                                            <div class="col-sm-9">
                                                <input id="phoneNumber" name="phoneNumber"
                                                       type="text" placeholder="PhoneNumber"
                                                        num-with-out-space maxlength="10"
                                                       ng-model="phone.phoneNumber"
                                                       class="form-control"/>
                                            </div>
                                            <div class="col-sm-1"
                                                 ng-if="phoneNumberList.length>1 && $index!=0">
                                                <input type="button" value="X" ng-click="removePhoneNumberList($index)">
                                            </div>
                                            <div class="col-sm-1"
                                                 ng-if="phoneNumberList.length<=2 && phoneNumberList.length-1==$index">
                                                <input type="button" value="+"
                                                       ng-click="addNewPhoneNumber()">
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <div class="col-md-6 col-sm-6 col-xs-12 form-group">
                                <div  ng-repeat="type in typeOfEnquiryList">
                                    <div class="col-sm-3" style="padding: 0px;">
                                        <label class="control-label" >Type Of Enquiry<span style="color: red">*</span></label>
                                    </div>
                                    <div class="col-sm-9" style="padding: 0px;margin-bottom: 10px;">

                                        <div class="col-md-12 col-sm-12 col-xs-12" style="padding: 0px;">
                                            <div class="col-sm-9">
                                                <select id="typeOfEnquiryList" name="typeOfEnquiryList" ng-model="type.typeOfEnquiry" ng-change="getServiceTypeList(type.typeOfEnquiry,type)" ng-options="type.name as type.name for type in enquiryTypeList" class="form-control input-md">
                                                    <option value="">Select</option>
                                                </select>
                                            </div>
                                            <div class="col-sm-1"
                                                 ng-if="typeOfEnquiryList.length>1 && $index!=0" >
                                                <input type="button" value="X" ng-click="removetypeOfEnquiryList($index)" style="margin-left: -15px;">
                                            </div>
                                            <div class="col-sm-1" style="margin-left: -15px"
                                            ng-if="typeOfEnquiryList.length-1==$index">
                                                <input type="button" value="+"
                                                       ng-click="addNewTypeOfEnquiry()">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-6 col-xs-12 form-group">
                                <div class="form-group" ng-repeat="type in typeOfEnquiryList">
                                <div  ng-show="type.typeOfEnquiry!='Others'">
                                    <div class="col-sm-3" style="padding: 0px;">
                                        <label class="control-label">Choose Service Type</label>
                                    </div>
                                    <div class="col-sm-9">
                                        <select id="serviceType" name="serviceType" ng-change="openInternationalHum(type,type.serviceType,$index)" ng-model="type.serviceType" ng-options="service.serviceType as service.serviceType for service in type.serviceTypeList" class="form-control input-md">
                                            <option value="">Select</option>
                                        </select>
                                    </div>
                                </div>
                                <div  ng-show="type.typeOfEnquiry=='Others'">
                                    <div class="col-sm-3" style="padding: 0px;">
                                        <label  class=" control-label">Others</label>
                                    </div>
                                    <div class="col-sm-9">
                                        <input id="others" name="others"
                                               type="text" placeholder="others"
                                               ng-model="others"
                                               class="form-control">
                                    </div>
                                </div>
                                </div>
                            </div>


                            <div class="col-md-12 col-sm-12 col-xs-12 form-group">
                                <div class="">
                                <div class="col-sm-4 col-xs-12">
                                    <div class="form-group">
                                        <label class="control-label col-sm-4" style="padding-left: 0px;">Country<span style="color: red">*</span></label>
                                        <div class="col-sm-8" style="padding: 0px 21px;">
                                            <select id="country" name="cuisines" ng-change="getStatewebList(country)" ng-model="country" ng-options="AAAA.countryId as AAAA.countryName for AAAA in countryList" class="form-control input-md">
                                                <option value="">Select</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-4 col-xs-12">
                                    <div class="form-group">
                                        <label class="control-label col-sm-4" >State<span style="color: red">*</span></label>
                                        <div class="col-sm-8">
                                            <select id="state" name="state" ng-change="getCitywebList(state)" ng-model="state" ng-options="BBBB.stateId as BBBB.stateName for BBBB in statewebListttt" class="form-control input-md">
                                                <option value="">Select</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-4 col-xs-12">
                                    <div class="form-group">
                                        <label class="control-label col-sm-4" >City<span style="color: red">*</span></label>
                                        <div class="col-sm-8" style=" margin-left: -32px;">
                                            <select id="city" name="city"   ng-model="city" ng-options="CCCC.cityId as CCCC.cityName for CCCC in citywebListttt" class="form-control input-md">
                                                <option value="">Select</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-6 col-xs-12 form-group">
                                <div class="col-sm-3" style="padding: 0px;">
                                    <label class="control-label" >Zipcode</label>
                                </div>
                                <div class="col-sm-9">
                                    <input id="Zipcode" name="Zipcode" num-with-out-space
                                           type="text" placeholder="Zipcode"
                                           ng-model="zipcode" maxlength="6"
                                           class="form-control">
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-6 col-xs-12 form-group">
                                <div class="col-sm-3" style="padding: 0px;">
                                    <label class="control-label" >Email<span style="color: red">*</span></label>
                                </div>
                                <div class="col-sm-9">
                                    <input id="emailId" name="emailId" ng-change="getEnquiryListBasedOn()"
                                           type="email" placeholder="Email" maxlength="50"
                                           ng-model="emailId" ng-pattern="word" ng-model-options="{debounce:1000}"
                                           class="form-control input-md">
                                    <p class="error" ng-show="enquiry.emailId.$error.pattern">Enter Valid MailID</p>
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-6 col-xs-12 form-group">
                                <div class="col-sm-3" style="padding: 0px;">
                                    <label class="control-label" >Call Status</label>
                                </div>
                                <div class="col-sm-9">
                                    <select name="custStatus" id="custStatus" ng-model="callStatus" ng-options="call.name as call.name for call in callStatusList" class="form-control" required>
                                        <option value="">Select</option>
                                        <!--<option value="Service_UnAvailable">Service_UnAvailable</option>-->
                                        <!--<option value="General_Enquiry">General_Enquiry</option>-->
                                        <!--<option value="Service_Initiated">Service_Initiated</option>-->
                                        <!--<option value="IVR_DROP">IVR_DROP</option>-->
                                    </select>
                                </div>
                            </div>
                        <div class="col-md-6 col-sm-6 col-xs-12 form-group" ng-show="callStatus=='General_Enquiry'">
                            <div class="col-sm-3" style="padding: 0px;">
                                <label class="control-label" >CallStatusDetails</label>
                            </div>
                            <div class="col-sm-9">
                                <select name="callStatusDetails" id="callStatusDetails" ng-model="callStatusDetails" class="form-control" required>
                                    <option value="">Select</option>
                                    <option value="perplanning">PRE-PLANNING</option>
                                    <option value="transportationOfAshes">Transportation of Ashes</option>
                                </select>
                            </div>
                        </div>
                            <div class="col-md-6 col-sm-6 col-xs-12 form-group">
                                <div class="col-sm-3" style="padding: 0px;">
                                    <label class="control-label" >Call Info</label>
                                </div>
                                <div class="col-sm-9">
                                     <textarea id="callinfo" name="callinfo"
                                               type="text" placeholder="Callinfo"
                                               ng-model="callinfo"
                                               class="form-control">
                                </textarea>
                                </div>
                            </div>
                        <div class="col-md-12 col-sm-12 col-xs-12 form-group">
                            <div  ng-repeat="special in specialRequestList">
                                <div class="col-sm-2" style="padding: 0px;">
                                    <label class="control-label" >Special Requirements</label>
                                </div>
                                <div class="col-sm-10" style="margin-left: -45px;padding: 0px;width: 85%;">
                                    <div class="col-md-12 col-sm-12 col-xs-12" style="padding: 0px;">
                                    <div class="col-sm-10" style="margin-bottom: 10px;">
                                    <textarea id="specialRequirements" name="specialRequirements"
                                    type="text" placeholder="Special Requirements"
                                    ng-model="special.specialRequirements"
                                    class="form-control"></textarea>
                                    </div>
                                    <div class="col-sm-1"
                                    ng-if="specialRequestList.length>1 && $index!=0" style="margin-left:0px;">
                                    <input type="button" value="X" ng-click="removeFile($index)">
                                    </div>
                                    <div class="col-sm-1" style="margin-left:-25px;" ng-if="specialRequestList.length-1==$index">
                                    <input type="button" value="+"
                                    ng-click="addNewFile()">
                                    </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                            <!--<div class="col-md-12 col-sm-12 col-xs-12 form-group">-->
                                <!--<div  ng-repeat="special in specialRequestList">-->
                                    <!--<div class="col-sm-3" style="padding: 0px;">-->
                                        <!--<label class="control-label" >Special Requirements</label>-->
                                    <!--</div>-->
                                    <!--<div class="col-sm-9" style="padding: 0px;margin-bottom: 10px;">-->

                                        <!--<div class="col-md-12 col-sm-12 col-xs-12" style="padding: 0px;">-->
                                            <!--<div class="col-sm-9">-->
                                                <!--<textarea id="specialRequirements" name="specialRequirements"-->
                                                <!--type="text" placeholder="Special Requirements"-->
                                                <!--ng-model="special.specialRequirements"-->
                                                          <!--class="form-control"></textarea>-->
                                            <!--</div>-->
                                            <!--<div class="col-sm-1"-->
                                                 <!--ng-if="specialRequestList.length>1 && $index!=0" >-->
                                                <!--<input type="button" value="X" ng-click="removeFile($index)">-->
                                            <!--</div>-->
                                            <!--<div class="col-sm-1">-->
                                                <!--<input type="button" value="+"-->
                                                       <!--ng-click="addNewFile()">-->
                                            <!--</div>-->
                                        <!--</div>-->
                                    <!--</div>-->
                                <!--</div>-->

                            <!--</div>-->

                    </div>
                </fieldset>



            </form>
        </div>

    </div>
    <div class="modal-footer" ng-show="(operation=='edit'||operation=='create')">
        <button type="button" class="btn btn-default" data-toggle="modal" ng-disabled="isDisabled"
                data-target=".print-me" ng-click="saveEnqForm('save&draft')">Save As Draft</button>
        <button type="submit" class="btn btn-default" data-toggle="modal" ng-disabled="isDisabled"
                data-target=".print-me" ng-click="saveEnqForm('save')">Save</button>
        <button type="button" class="btn btn-default" data-dismiss="modal" ng-click="goBack()">Close</button>

    </div>
    <div class="modal-footer" ng-show="operation=='addCall'">
        <button type="submit" class="btn btn-default" data-toggle="modal" ng-disabled="isDisabled"
                data-target=".print-me" ng-click="saveEnqFormAddCall()">Save</button>
        <button type="button" class="btn btn-default" data-dismiss="modal" ng-click="goBack()">Close</button>

    </div>
</div>
<div class="modal fade" id="add_new_hearse_Van" >
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span></button>
                <h4 id="country-title">Hearse Van Service</h4>
            </div>
            <div class="modal-body">
                <div class="box-body">
                    <div class="row" id="cus_step1">
                        <div class="form-group">
                            <label class="control-label col-sm-3">Date of Service</label>
                            <div class="col-sm-9 mb-15">
                                <div class="input-group date">
                                    <input type="text" class="form-control" uib-datepicker-popup="{{format}}"
                                           ng-model="serviceTypeDetails.dateOfServicehv"
                                           is-open="popup1.opened" readonly datepicker-options="dateOptions"
                                           ng-required="true"
                                           close-text="Close" alt-input-formats="altInputFormats"/>
                                    <span class="input-group-btn">
                                    <button type="button" class="btn btn-default" ng-click="open1()"><i
                                            class="glyphicon glyphicon-calendar"></i></button>
                                    </span>
                                </div>
                            </div>
                        </div>
                        <div class="form-group" >
                            <label class="control-label col-sm-3">Time Of Service</label>
                            <div class="col-sm-9 mb-15" >
                                <input id="timeOfService" name="timeOfService"
                                       type="time" placeholder="Time"
                                       ng-model="serviceTypeDetails.timeOfServicehv"
                                       class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-3">Cost Of Service(INR)</label>
                            <div class="col-sm-9 mb-15">
                                <input id="costOfService" name="costOfService" num-with-out-space
                                       type="text" placeholder="ex:23"
                                       ng-model="serviceTypeDetails.costOfServicehv" maxlength="6"
                                       class="form-control">
                            </div>
                        </div>
                        <h4><strong>Pick Up Address</strong><span style="color: red">*</span></h4>
                        <div class="form-group">
                            <label class="control-label col-sm-3"> Street Address</label>
                            <div class="col-sm-9 mb-15">
                                <input id="pickupAddress" name="pickupAddress"
                                       type="text" placeholder="Pickup Address"
                                       ng-model="serviceTypeDetails.pickupAddresshv"
                                       class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
