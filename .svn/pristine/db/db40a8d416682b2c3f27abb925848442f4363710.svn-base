<div class="modal fade" id="add_new" >
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true" ng-click="closeData()">&times;</span></button>
                <h4 id="country-title">{{servceName}}</h4>
            </div>
            <div class="modal-body">
                <div class="box-body">
                    <div class="row" id="cus_step1">

                        <div class="form-group" ng-repeat="key in keyList" ng-init="parentIndex=$index">
                            <!--<div class="form-group" ng-repeat="val in keyValuesList">-->
                            <div class="col-lg-3 col-md-12 col-sm-12 col-xs-12" ng-show="key.type=='date'">
                                <label>{{key.field}}</label>
                            </div>
                            <div class="col-lg-9 col-md-12 col-sm-12 col-xs-12 input-group date" ng-show="key.type=='date'">
                                <p class="input-group" style="width: 35%;">
                                    <input type="text" class="form-control" uib-datepicker-popup="{{format}}"
                                           ng-model="key.values" style="height: 35px;"
                                           is-open="popup1.opened" readonly datepicker-options="dateOptions"
                                           ng-required="true"
                                           close-text="Close" alt-input-formats="altInputFormats"/>
                                    <span class="input-group-btn">
                                     <button type="button" class="btn btn-default" ng-click="open1()"><i
                                             class="glyphicon glyphicon-calendar"></i></button>
                                </span>
                                </p>
                            </div>
                            <div class="col-lg-3 col-md-12 col-sm-12 col-xs-12" ng-show="key.type=='input'" >
                                <label>{{key.field}}</label>
                            </div>
                            <div class="col-lg-9 col-md-12 col-sm-12 col-xs-12 input-group date" ng-show="key.type=='input'">
                                <input id="input" name="input"
                                       type="text"
                                       ng-model="key.values" maxlength="125"
                                       class="form-control">
                            </div>
                            <div class="col-lg-3 col-md-12 col-sm-12 col-xs-12" ng-show="key.type=='textarea'" >
                                <label>{{key.field}}</label>
                            </div>
                            <div class="col-lg-9 col-md-12 col-sm-12 col-xs-12 input-group date" ng-show="key.type=='textarea'">
                                <textarea id="input" name="input"
                                          type="text"
                                          ng-model="key.values" maxlength="125"
                                          class="form-control">
                                </textarea>
                            </div>
                            <div class="col-lg-3 col-md-12 col-sm-12 col-xs-12" ng-show="key.type=='radio'" >
                                <label>{{key.field}}</label>
                            </div>
                            <div class="col-lg-9 col-md-12 col-sm-12 col-xs-12 input-group date" ng-show="key.type=='radio'" ng-repeat="val in key.valueList">
                                <div class=" col-sm-9" >
                                    <input id="serviceLift" name="key.values{{$index}}" type="radio" ng-value="val.value"   ng-model="key.values">{{val.value}}
                                </div>
                            </div>
                            <div class="col-lg-3 col-md-12 col-sm-12 col-xs-12" ng-show="key.type=='checkbox'" >
                                <label>{{key.field}}</label>
                            </div>
                            <div class="col-lg-9 col-md-12 col-sm-12 col-xs-12 input-group date" ng-show="key.type=='checkbox'" ng-repeat="val in key.valueList">
                                <div class=" col-sm-9">
                                    <input id="checkbox" name="key.values" type="checkbox" value="val.value"  ng-model="val.values" >{{val.value}}
                                </div>
                            </div>
                            <div class="col-lg-3 col-md-12 col-sm-12 col-xs-12"  ng-show="key.type=='select'" >
                                <label>{{key.field}}</label>
                            </div>
                            <div class="col-lg-9 col-md-12 col-sm-12 col-xs-12 input-group date" ng-show="key.type=='select'">
                                <div class=" col-sm-9">
                                    <select name="assistanceDetails" id="assistanceDetails" ng-options="s.value as s.value for s in key.valueList" ng-model="key.values" class="form-control" required>
                                        <option value="">Select</option>
                                        <option value="key.value">{{key.value}}</option>
                                    </select>
                                </div>
                            </div>
                        </div>

                    </div>

                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-toggle="modal" ng-disabled="isDisabled"
                        data-target=".print-me" ng-click="saveFields()">Save</button>
                <button type="button" class="btn btn-default" ng-click="closeData()" data-dismiss="modal">Close</button>

            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
