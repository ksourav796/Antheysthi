<section class="content-header">
    <h1>
        Franchise
    </h1>

</section>

<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="top-box">

                <div class="col-md-10">
                    <div class="top-button-area">
                        <div class="col-md-2 col-sm-4 col-xs-4">
                            <button type="button"  class="dropbtn btn btn-primary hi_btn" ng-click="addFranchise()"style="width:80%">Add</button>
                        </div>
                        <div class="input-group">
                            <input class="form-control" placeholder="Search..." id="textinput" name="textinput"  type="text"
                                   placeholder="Search Country" ng-model="franchSearch"
                                   ng-change="getPaginationList(countrySearchText)">

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="content">
    <div class="row">
        <div class="col-md-12 col-xs-12 customer-data" style="overflow-x:auto">
            <table id="example" class="table table-striped table-bordered nowrap" style="width:100%">
                <thead>
                <tr>
                    <th>
                        FranchiseCode
                    </th>
                    <th>
                        FranchiseName
                    </th>
                    <th>
                        Address
                    </th>
                    <th>
                        Actions
                    </th>
                </tr>


                </thead>
                <tbody>
                <tr ng-repeat="franchise in franchiseList| filter:franchSearch">
                    <td>{{franchise.franchiseCode}}</td>
                    <td>{{franchise.franchiseName}}</td>
                    <td>{{franchise.franchiseAddress}}</td>

                    <td><button type="button" class="btn btn-primary hi_btn"  ng-click="editFranchise(franchise)">Edit</button></td>
                    <!--<button type="button" class="btn btn-primary hi_btn"  ng-click="deletecustomer(customer.customerId)">Delete</button></td>-->
                </tr>
                </tbody>
            </table>
    </div>
    </div>
</section>

