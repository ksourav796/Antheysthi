</section>
<div class="modal fade" id="add_new_res_modal">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span></button>
                <h4 id="res-title">Add Enquiry</h4>
            </div>
            <div class="modal-body">
                <div class="box-body">
                    <div class="row" id="cus_step1">
                        <form class="form-horizontal" name="student">
                        <div class="form-group" >
                            <label class="col-sm-3 control-label">Agency Name</label>
                            <div class="col-sm-9">
                                <input  type="text" id="parent" name="parent" ng-model="parent" readonly  class="form-control input-md"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Product<span style="color: red">*</span></label>
                            <div class="col-sm-9 " style="position:relative;">
                                <select  name="product" ng-model="categoryId" ng-options="category.categoryName
                                as category.categoryName for category in categoryList"
                                         ng-change="CategoryBasedOnService(categoryId)"
                                         class="form-control input-md ng-change='getFirstLevel(currencyId)">
                                    <option value="">Select</option>
                                </select>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-sm-3  control-label">Service<span style="color: red">*</span></label>
                            <div class=" col-sm-9" style="position:relative;">
                                <select  name="service" ng-model="service" ng-options="service.name
                                as service.name for service in serviceList"
                                         class="form-control input-md ng-change='getFirstLevel(currencyId)">
                                    <option value="">Select</option>
                                </select>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-sm-3  control-label">User<span style="color: red">*</span></label>
                            <div class=" col-sm-9" style="position:relative;">
                                <select  name="user" ng-model="user" ng-options="user.userName
                                as user.firstName for user in userList"
                                         class="form-control input-md ng-change='getFirstLevel(currencyId)">
                                    <option value="">Select</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label  class="col-sm-3 control-label">Customer Name<span style="color: red">*</span></label>
                            <div class="col-sm-9">
                                <input id="Rname" name="Name" alphanum-with-space=""
                                       type="text" placeholder="Customer Name"
                                       ng-model="Name" maxlength="125"
                                       class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label  class="col-sm-3 control-label">Restaurant Name<span style="color: red">*</span></label>
                            <div class="col-sm-9">
                                <input id="Rname" name="Rname" alphanum-with-space=""
                                       type="text" placeholder="Restaurant Name"
                                       ng-model="Rname" maxlength="125"
                                       class="form-control">
                            </div>
                        </div>
                            <div class="form-group">
                                <label class="col-sm-3 control-label"
                                       for="">Email <span style="color:red;">*</span></label>
                                <div class="col-sm-9" style="position:relative;">
                                    <input id="emailId" name="emailId"
                                           type="email" placeholder="Email" maxlength="50"
                                           ng-model="Emailid" ng-pattern="word"
                                           class="form-control input-md">
                                    <p class="error" ng-show="student.emailId.$error.pattern">Enter Valid MailID</p>
                                </div>
                            </div>
                        <!--<div class="form-group">-->
                            <!--<label  class="col-sm-3 control-label">Email<span style="color: red">*</span></label>-->
                            <!--<div class="col-sm-9">-->
                                <!--<input id="Email" name="Email"-->
                                       <!--type="text" placeholder="Email"-->
                                       <!--ng-model="Emailid" maxlength="125"-->
                                       <!--class="form-control">-->
                            <!--</div>-->
                        <!--</div>-->

                        <!--<div class="form-group">-->
                            <!--<label  class="col-sm-3 control-label">Email<span style="color: red">*</span></label>-->
                            <!--<div class="col-sm-9">-->
                                <!--<input id="Email" name="Email"-->
                                       <!--type="text" placeholder="Email"-->
                                       <!--ng-model="Emailid" maxlength="125"-->
                                       <!--class="form-control">-->
                            <!--</div>-->
                        <!--</div>-->
                        <div class="form-group">
                            <label  class="col-sm-3 control-label">Contact No<span style="color: red">*</span></label>
                            <div class="col-sm-9">
                                <input id="Contact" name="Contact" num-with-out-space=""
                                       type="text" placeholder="Contact"
                                       ng-model="Contact" maxlength="10"
                                       class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3  control-label">Country<span style="color: red">*</span></label>
                            <div class=" col-sm-9" style="position:relative;">
                                <select id="country" name="cuisines" ng-change="getStatewebList(country)" ng-model="country" ng-options="AAAA.countryId as
AAAA.countryName for AAAA in countryList" class="form-control input-md">
                                    <option value="">Select</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3  control-label">State<span style="color: red">*</span></label>
                            <div class=" col-sm-9" style="position:relative;">
                                <select id="state" name="state" ng-change="getCitywebList(state)" ng-model="state" ng-options="BBBB.stateId as
BBBB.stateName for BBBB in statewebListttt" class="form-control input-md">
                                    <option value="">Select</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3  control-label">City<span style="color: red">*</span></label>
                            <div class=" col-sm-9" style="position:relative;">
                                <select id="city" name="city"  ng-change="getLocationwebList(city)"  ng-model="city" ng-options="CCCC.cityId as
CCCC.cityName for CCCC in citywebListttt" class="form-control input-md">
                                    <option value="">Select</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3  control-label">Location<span style="color: red">*</span></label>
                            <div class=" col-sm-9" style="position:relative;">
                                <select id="location" name="Location" ng-model="location" ng-options="Location.location_name as
Location.location_name for Location in locationwebListttt" class="form-control input-md">
                                    <option value="">Select</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label  class="col-sm-3 control-label">Company</label>
                            <div class="col-sm-9">
                                <input id="company" name="company" alpha-with-space=""
                                       type="text" placeholder="company"
                                       ng-model="Rname" maxlength="125"
                                       class="form-control">
                            </div>
                        </div>
                            <div class="form-group">
                            <label  class="col-sm-3 control-label">Enter Url<span style="color: red">*</span></label>
                            <div class="col-sm-6">
                                <input id="slug" name="slug" alphanum-without-space="" ng-change="CheckUrlAvailability(slug)"
                                       type="text" placeholder=" " ng-model-options="{debounce:500}"
                                       ng-model="slug" maxlength="50"
                                       class="form-control">
                                <p class="error" ng-show="urlValue" style="color: #0b93d5">{{message}}</p>
                            </div>.eatinng.com
                        </div>
                        <div class="form-group">
                            <label  class="col-sm-3 control-label">Address1<span style="color: red">*</span></label>
                            <div class="col-sm-9">
                                <input id="Address1" name="address1"
                                       type="text" placeholder="Address"
                                       ng-model="address1" maxlength="225"
                                       class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label  class="col-sm-3 control-label">Address2<span style="color: red">*</span></label>
                            <div class="col-sm-9">
                                <input id="Address1" name="address2"
                                       type="text" placeholder="Address"
                                       ng-model="address2" maxlength="225"
                                       class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label  class="col-sm-3 control-label">Zipcode</label>
                            <div class="col-sm-9">
                                <input id="Zipcode" name="Zipcode" num-with-out-space=""
                                       type="text" placeholder="Zipcode"
                                       ng-model="Zipcode" maxlength="125"
                                       class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label" for="">Status</label>
                            <div class="col-sm-9" >
                                <select name="status" id="status" ng-model="custstatus"   class="form-control input-md">
                                    <option value="">Select</option>
                                    <option value="Interested">Interested</option>
                                    <option value="NotInterested">NotInterested</option>
                                    <option value="Rejected">Rejected</option>
                                    <option value="callBack">callBack</option>

                                </select>
                            </div>
                        </div>
                        <div ng-show="custstatus=='callBack'" class="form-group">
                            <label  class="col-sm-3  control-label"  for="">CallBack Date:</label>
                            <div class="col-sm-9">
                                <p class="input-group">
                                    <input type="text" class="form-control"  uib-datepicker-popup="{{format}}"  readonly ng-model="callbackDate" is-open="popupE.opened" datepicker-options="dateOptions" ng-required="true" close-text="Close" alt-input-formats="altInputFormats" />
                                    <span class="input-group-btn">
                                     <button type="button" class="btn btn-default" ng-click="openE()"><i class="glyphicon glyphicon-calendar"></i></button>
                                </span>
                                </p>
                            </div>
                        </div>
                        <div ng-show="custstatus=='callBack'"  class="form-group">
                            <label  class="col-sm-3 control-label">Time</label>
                            <div class="col-sm-9">
                                <input id="Time" name="Time" num-with-out-space=""
                                       type="text" placeholder="Time"
                                       ng-model="callbacktime" maxlength="125"
                                       class="form-control">
                            </div>
                        </div>
