<section class="content-header">
    <h1>
        Enquiry Form
    </h1>

</section>

<!-- Main content -->
<section class="content">
    <!-- Small boxes (Stat box) -->
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="top-box">
                <div class="col-md-10">
                    <div class="top-button-area">
                        <div class="col-md-2 col-sm-4 col-xs-4" ng-show="userAccessRights.enqAdd">
                            <button type="button"  class="dropbtn btn btn-primary hi_btn" ng-click="addCall('Create')" style="width:80%">Add</button>
                        </div>
                        <!--<div class="col-md-2 col-sm-4 col-xs-4 ">-->
                            <!--<button type="button"  class="dropbtn btn btn-primary hi_btn" id="colButnw"  data-toggle="modal"-->
                                    <!--ng-model="StatusText" data-target=".print-me"-->
                                    <!--ng-click="inactiveButton()">{{ButtonStatus}}</button>-->
                        <!--</div>-->
                        <!--<div class="col-md-2 col-sm-4 col-xs-4">-->
                            <!--<button type="button" class="dropbtn btn btn-primary hi_btn" ng-click="goBack()">Back</button>-->
                        <!--</div>-->

                    </div>

                    <div class="col-sm-4 col-xs-12 input-group">
                        <input class="form-control mbser" placeholder="Search First Name..." id="textinput" name="textinput"  type="text"
                               ng-model="SearchText" ng-model-options="{debounce:500}"
                               ng-change="getPaginationList(SearchText)">

                    </div>


                </div>
            </div>
        </div>
    </div>
</section>

<section class="content">
    <div class="row">
        <div class="col-md-12 col-xs-12 customer-data">
            <table id="example" class="table table-striped table-bordered nowrap" style="width:100%">
                <thead>
                <tr>

                    <th>
                         Date
                    </th>
                    <th>
                        Enquiry Form No
                    </th>
                    <th>
                        Caller Location
                    </th>
                    <th>
                        Source Of enquiry
                    </th>
                    <th>
                        First Name
                    </th>
                    <th>
                        Status
                    </th>
                    <th>
                        Call List
                    </th>
                    <th>
                        Actions
                    </th>
                </tr>


                </thead>
                <tbody>
                <tr ng-repeat="enquiry in enquiryList| filter:SearchText">
                    <td>{{enquiry.enquiryDate|date:'dd/MM/yyyy'}}</td>
                    <td>{{enquiry.enquiryNo}}</td>
                    <td>{{enquiry.callerLocation}}</td>
                    <td>{{enquiry.sourceOfEnquiry}}</td>
                    <td>{{enquiry.firstName}}</td>
                    <td>{{enquiry.callStatus}}</td>
                    <!--<td>{{enquiry.assignstatus}}</td>-->
                    <td></td>
                    <td>
                        <div class="dropdown" ng-show="userAccessRights.enqMore">
                            <button   ng-click="myFunction(enquiry)" class="dropbtn btn btn-primary hi_btn">More</button>
                            <div id="myDropdown{{enquiry.id}}" class="dropdown-content">
                                <table style="border:none; width:100%;" >
                                    <tr>
                                        <td style="border:none;" ng-show="userAccessRights.enqAddcall">
                                            <button type="button" class="btn btn-default hi_btn"  ng-click="addCall('addCall')">Add Call</button>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td style="border:none;" ng-show="userAccessRights.enqConfirmEnquiry">
                                            <button type="button" class="btn btn-default hi_btn" ng-click="confirmenquiry(enquiry)" >Confirm Enquiry</button>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td style="border:none;" ng-show="userAccessRights.enqEdit">
                                            <button type="button" class="btn btn-default hi_btn" ng-click="addCall('edit')" >Edit</button>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td style="border:none;" ng-show="userAccessRights.enqAssign">
                                            <button type="button" class="btn btn-default hi_btn" ng-click="assign()">{{assignstatus}}</button>
                                        </td>
                                    </tr>
                                </table>
                            </div>
                        </div>
                    </td>



                    <!--<td>-->
                    <!--<button type="button" class="btn btn-primary hi_btn"  ng-click="editEnquiry(enquiry)">Edit</button>-->
                    <!--<button type="button" class="btn btn-primary hi_btn"  ng-click="editEnquiry(enquiry)">Add Call</button></td>-->
                    <!--<button type="button" class="btn btn-primary hi_btn"  ng-click="deletecustomer(customer.customerId)">Delete</button></td>-->
                </tr>
                </tbody>
            </table>
            <div class="col-sm-12 pagination_">

                <div class="col-sm-12" style="float:right !important;">
                    <button type="button" class="btn btn-default hi_btn" ng-class="{'diabled-class': first}"
                            ng-disabled="first"
                            ng-click="getPaginationList('firstPage')" ng-model="first">First
                    </button>
                    <button type="button" class="btn btn-default hi_btn" ng-class="{'diabled-class': prev}"
                            ng-disabled="prev"
                            ng-click="getPaginationList('prevPage')" ng-model="prev">Prev
                    </button>
                    <button type="button" class="btn btn-default hi_btn" name="next" ng-disabled="next"
                            ng-click="getPaginationList('nextPage')" value="next"
                            ng-class="{'diabled-class': next}" ng-model="next">Next
                    </button>
                    <button type="button" class="btn btn-default hi_btn"
                            ng-click="getPaginationList('lastPage')" ng-class="{'diabled-class': last}"
                            ng-disabled="last"
                            ng-model="last">Last
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>

<style>

    .dropdown {
        position: relative;
        display: inline-block;
    }

    .dropdown-content {
        display: none;
        position: absolute;
        background-color: #fff;
        min-width: 176px;
        overflow: auto;
        z-index: 1;
        border: 1px solid #ccc;
        border-color: rgba(0,0,0,.2);
        padding: 10px;
        right: -43px;
        /* left: 0px; */
        top: 42px;
    }
    .dropdown-content button{
        width:100%;
        margin-top:10px;
        border-radius: 0px;
        padding: 10px 10px;
        font-size: 19px;
        text-align:left;
    }

    .show {display: block;}
</style>
<script>
    window.onclick = function(event) {
        if (!event.target.matches('.dropbtn')) {
            var dropdowns = document.getElementsByClassName("dropdown-content");
            var i;
            for (i = 0; i < dropdowns.length; i++) {
                var openDropdown = dropdowns[i];
                if (openDropdown.classList.contains('show')) {
                    openDropdown.classList.remove('show');
                }
            }
        }
    }
</script>
